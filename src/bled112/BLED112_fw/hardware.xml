<?xml version="1.0" encoding="UTF-8" ?>

<hardware>
    <sleeposc enable="false" ppm="30" />
    <usb enable="true" endpoint="api" />
    <!-- BLE112: 15 is the highest TX power setting and equals roughly to +3dBm while 0 is the lowest value and corresponds to around -24dBm -->
    <txpower power="15" bias="5" />
    <port index="0" tristatemask="0" pull="down" />
    <port index="1" tristatemask="0" pull="down" />
    <port index="2" tristatemask="0" pull="down" />
</hardware>
